{
  "name": "quick-ui-react",
  "version": "1.0.6",
  "description": "A modern React component library built with TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "default": "./dist/style.css"
    },
    "./dist/style.css": {
      "types": "./dist/styles.d.ts",
      "default": "./dist/style.css"
    }
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "analyze": "set ANALYZE=true&& vite build && start dist/stats.html",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest run --watch",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "typescript",
    "component-library",
    "react-components",
    "ui-library",
    "design-system"
  ],
  "author": {
    "name": "Echegini, Ngozi",
    "email": "silasngoziechegini@gmail.com",
    "url": "https://example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silasechegini/Quick-UI.git"
  },
  "homepage": "https://github.com/silasechegini/Quick-UI#readme",
  "bugs": {
    "url": "https://github.com/silasechegini/Quick-UI/issues"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.5.1",
    "focus-trap-react": "^11.0.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-vitest": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "clsx": "^2.1.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jiti": "^2.5.1",
    "jsdom": "^27.2.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "react-docgen-typescript": "^2.4.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.94.0",
    "storybook": "^10.0.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
